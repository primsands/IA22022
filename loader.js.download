function(box, data) {
    try {
        /**
         * A complete hack to replace the TRC URL which was behaving
         * different and throwing error.
         */
        var trcUrlHack = data['logger_url'],
            itemThumbnailHrefHack = box.getElementsByClassName('item-thumbnail-href')[0],
            itemLabelHrefHack = box.getElementsByClassName('item-label-href')[0],
            videoLabelBoxHack = box.getElementsByClassName('video-label-box')[0],
            thumbBlockHolderHack = box.getElementsByClassName('thumbBlock_holder')[0];
        var anchorTempThumbnailHack = document.createElement('a');
        anchorTempThumbnailHack.setAttribute('href', trcUrlHack);
        anchorTempThumbnailHack.setAttribute('target', '_blank');
        anchorTempThumbnailHack.appendChild(thumbBlockHolderHack);
        box.appendChild(anchorTempThumbnailHack);
        var anchorLabelHrefHack = document.createElement('a');
        anchorLabelHrefHack.setAttribute('href', trcUrlHack);
        anchorLabelHrefHack.setAttribute('target', '_blank');
        anchorLabelHrefHack.appendChild(videoLabelBoxHack);
        box.insertBefore(anchorLabelHrefHack, itemThumbnailHrefHack[1]);
        //
        //  Sunsern.d Nov 6, 2019
        //  IE11 doesn't support remove(), using removeChild(...) instead
        //
        box.removeChild(itemThumbnailHrefHack);
        box.removeChild(itemLabelHrefHack);
        // itemThumbnailHrefHack.remove();
        // itemLabelHrefHack.remove();
        anchorTempThumbnailHack.className += 'item-thumbnail-href';
        anchorLabelHrefHack.className += 'item-label-href';
        /**
         * ----------------------------------------
         */
    }catch (e) {
        __trcError('Error in item-renderer (thumbnails-bc-amp-native) : ' + e.message);
    }
}
