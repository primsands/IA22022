function(id) {
    /**
     * Done by rajendra.a
     * Manually Start Video Event when pub video is closed by clicking the close button
     */
    /***** Start - publisher-end *****/
    try {
        /**
         * Have added, just in case if there is a player.
         * For.g: in whimn, there is no player so it will fall into
         * else condition.
         */
        var btnClose = document.getElementsByClassName('atom-videoplayer-sticky-close')[0];
        var divVideo = document.querySelector('.atom-videoplayer.vm-sticky');
        if (divVideo){
            if(btnClose){
                if(!TRC.bVideoStarted){
                    // console.log('Binding-onclick');
                    btnClose.addEventListener('click', function(){
                        // When user clicks the publisher's close button, start our video.
                        if(TRC.videoFunctions) {
                            console.log('Start Video Event');
                            TRC.bVideoStarted = true;
                            //setTimeout(function () { TRC.videoFunctions.startEvent(); }, 2000);
                            TRC.Timeout.set(TRC.videoFunctions.startEvent, 2000); // Use existing function
                        }
                        return true;
                    });
                }
            }
        } else {
            // If there is no Pub video, start video event now
            // console.log('no pub video');
            if(TRC.videoFunctions && !TRC.bVideoStarted){
                console.log('Start Video Event');
                TRC.bVideoStarted = true;
                //setTimeout(function(){ TRC.videoFunctions.startEvent(); }, 2000);
                TRC.Timeout.set(TRC.videoFunctions.startEvent, 2000); // Use existing function
            }
        }
    } catch (e) {
        __trcError('Error in publisher-end ' + e.message);
    }
    /***** END - publisher-end *****/
}
