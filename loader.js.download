function(){
    //  Done by rajendra.a
    //  The variable is used as a flag to check if our video is started or not
    //
    TRC.bVideoStarted = false;
    /**
     * Done by Dor.S , 27.1.2019 , TS-124044
     * Create mapping for the labels.
     * Invoking from item-render at OC cards.
     * @param data
     * @param box
     */
    TRC.categoryColorMapping = function(data,box){
        try{
            var url = data.url,
                labelDiv = document.createElement('div'),
                videoLabelBox = box.getElementsByClassName('video-label-box')[0];
            if(videoLabelBox && url) {
                var labelName = url.split('/')[3],
                    subCategory = url.split('/')[4];

                if(data && (data['is-native'] === true) || data['is-native'] == 'true') {
                    labelName = labelName.replaceAll('-', ' ');
                    labelDiv.innerText = labelName;
                } else {

                    // check that the sub-category is not long string (s.a the article title)
                    if ((subCategory.split('-').length - 1) <= 1) {
												if(subCategory) {
													subCategory = subCategory.replaceAll('-', ' ');
												}
                        labelDiv.innerText = subCategory;
                    } else {
												if(labelName) {
													labelName = labelName.replaceAll('-', ' ');
												}
                        labelDiv.innerText = labelName;
                    }
                }
                labelDiv.classList.add('video-label-added-label');
                labelDiv.style.color = '#999';
                videoLabelBox.insertAdjacentElement('afterbegin', labelDiv);
            }
        }
        catch (err){
            __trcError('Error in publisher-start: TRC.categoryColorMapping ' + err.message);
        }
    }
    /**
     * Done by Dor.S , 30.12.2018 , TS-123817
     * Adding font awsome to the site
     */
    TRC.setFontAwsome = function(){
        try{
            var linkElement = document.createElement('link');
            linkElement.rel = 'stylesheet';
            linkElement.href = 'https://use.fontawesome.com/releases/v5.6.3/css/all.css';
            linkElement.setAttribute('integrity','sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/');
            linkElement.crossOrigin = 'anonymous';
            document.getElementsByTagName('head')[0].appendChild(linkElement);
        }
        catch (err){
            __trcError('Error in publisher-start: TRC.setFontAwsome ' + err.message);
        }
    }();
    /**
     * Done by Dor.S , 30.12.2018 , TS-123817
     * Add the fomnt-awsome icon
     * Invoking fron item-render
     * @param classArr - array of class to add. Make sure that 2 classes bekong to the font-awsome icon
     * @param element - the element to appand the font
     * @param insertAdjacentElementPosition - the position to use for 'insertAdjacentElement' function.
     */
    TRC.setFontAwsomeClass = function(classArr,element,insertAdjacentElementPosition){
        try{
            if(element instanceof HTMLElement && classArr instanceof Array){
                var iElement = document.createElement('i');
                classArr.forEach(function (c) {
                    iElement.classList.add(c);
                });
                element.insertAdjacentElement(insertAdjacentElementPosition,iElement);
            }
            else {
                console.info('Check params in TRC.setFontAwsomeClass, ABORTING');
                return;
            }
        }
        catch (err){
            __trcError('Error in publisher-start: TRC.setFontAwsomeClass ' + err.message);
        }
    }
	
			/**
     * Done by Dor.S , 24.1.2019 , TS-123817
     * Add the fomnt-awsome icon, Or image (NCA requested SVG)
     * Invoking fron item-render
     * @param classArrOrLinkFromZinkSVG - array of class to add. Make sure that 2 classes belong to the font-awsome icon
     * OR the cdn link
     * @param element - the element to appand
     * @param insertAdjacentElementPosition - the position to use for 'insertAdjacentElement' function.
     */
    TRC.setFontAwsomeOrSVGImageClass = function (classArrOrLinkFromZinkSVG, element, insertAdjacentElementPosition) {
        if (typeof classArrOrLinkFromZinkSVG === 'string' && element instanceof HTMLElement) {
            try {
                var span = document.createElement('span');
                span.style.backgroundImage = 'url(' + classArrOrLinkFromZinkSVG + ')';
                span.style.backgroundRepeat = 'no-repeat';
                span.classList.add('added-icon-svg')
                element.insertAdjacentElement(insertAdjacentElementPosition, span);
            } catch (e) {
                __trcError('TRC.setFontAwsomeOrSVGImageClass, at linkFromZinkSVG condition: ', e.message);
            }
        } else {
            console.info('Check params in TRC.setFontAwsomeOrSVGImageClass, ABORTING');
            return;
        }
    }
	
    /**
     * Done by Dor.S,  6.1.2019 ,TS-123817
     * Reording the brandig to be: <Promoted> | <branding>
     * Invoking fron item-render
     * @param brandinSpan
     */
    TRC.reorderBranding = function(brandinSpan){
        try {
            var sponsoredDiv = brandinSpan.getElementsByClassName('logoDiv')[0],
                brandingInner = brandinSpan.getElementsByClassName('branding-inner')[0];
            if (brandingInner && sponsoredDiv) {
                brandinSpan.insertAdjacentElement('afterbegin',sponsoredDiv);
                brandinSpan.insertAdjacentElement('beforeend',brandingInner);
            }
        }
        catch (err){
            __trcError('Error in publisher-start: TRC.reorderBranding ' + err.message);
        }
    }
    /**
     * Move elemnts in the mode
     * @param parent - parent contanier
     * @param child - element to move
     * @param insertAdjacentElementPosition
     */
    TRC.moveElements = function(parent,child,insertAdjacentElementPosition){
        try{
            if(parent && child){
                parent.insertAdjacentElement(insertAdjacentElementPosition,child);
            }
        }
        catch (e){
            __trcError('Error in TRC.moveElements: ' + e.message);
        }
    }
    /**
     * Created by Dor.s , May 5th 2019
     * Modified by Raj, Jun 11th 2019
     * Applied to NCA sites by Napapan.n, May 11th 2019
     * Change the Host for native cards
     * Invoked from item-data-filter
     * @param data
     */
    TRC.changeHostForNative = function(data) {
        try {
            /**
             * PlnnGroup url list.
             */
            var allowedPlnnGroup = ['amp.heraldsun.com.au',
                    'amp.couriermail.com.au',
                    'amp.dailytelegraph.com.au',
                    'amp.adelaidenow.com.au',
                    'amp.goldcoastbulletin.com.au',
                    'amp.ntnews.com.au',
                    'amp.townsvillebulletin.com.au',
                    'amp.weeklytimesnow.com.au',
                    'amp.geelongadvertiser.com.au',
                    'amp.cairnspost.com.au',
                    'amp.themercury.com.au',
                    'www.heraldsun.com.au',
                    'www.couriermail.com.au',
                    'www.dailytelegraph.com.au',
                    'www.adelaidenow.com.au',
                    'www.goldcoastbulletin.com.au',
                    'www.ntnews.com.au',
                    'www.townsvillebulletin.com.au',
                    'www.weeklytimesnow.com.au',
                    'www.geelongadvertiser.com.au',
                    'www.cairnspost.com.au',
                    'www.themercury.com.au'],
                iFrameDetection = (window === window.parent) ? false : true,
                currentHost = iFrameDetection ? document.referrer.split('/')[2] : window.location.hostname;
            if (data['publisher'] && data['publisher'].indexOf('native') > -1 && allowedPlnnGroup.indexOf(data['url'].split('/')[2]) > -1) {
                var regex = /w{3}.[a-z]+.com.au/;
                var newURL;
                if(iFrameDetection) {
                    var hostname = data['url'].split('/')[2];
                    var newFromedUrl = data['url'].replace(hostname, currentHost);
                    var newRedirectionUrl = newFromedUrl.replace(/[^.]*/, 'https://www');
                    newURL = newRedirectionUrl;
                } else {
                    newURL = data['url'].replace(regex, currentHost);
                }
                console.log('newURL: ', newURL)
                data['url'] = newURL;
            }
        } catch (e) {
            __trcError('Error at TRC.changeHostForNative: ', e.message);
        }
    }
    /**
     * Done by Sunsern.d 12.11.2019
     * Modified by Sunsern.d 31.03.2020
     * Force static organic contents for a backfill item
     * The idea is we're gonna random one of static OCs from 5 fixed items
     * @param box
     * @return trcUrlHack
     */
    TRC.randomFixedOCs = function(box) {
        try {
            var thumbBlockHolderHack = box.getElementsByClassName('thumbBlock_holder')[0];
            //
